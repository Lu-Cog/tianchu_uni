.btn {
  padding: 12rpx 0;
  text-align: center;
  width: 90%;
  background: #0dce97;
  border-radius: 32rpx;
  color: #fff;
  position: fixed;
  bottom: 50rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.header {
  width: 100%;
  box-shadow: 0px 2px 6px rgba(0, 204, 146, 0.1);
}
.header .head {
  position: relative;
  padding: 30rpx;
}
.header .head image {
  position: absolute;
  left: 30rpx;
  width: 50rpx;
  height: 50rpx;
}
.header .head text {
  font-size: 36rpx;
  font-weight: 400;
}
.header .head navigator {
  position: absolute;
  right: 30rpx;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  color: #0dce97;
  font-size: 26rpx;
  font-weight: 700;
}
.leavelist {
  padding: 48rpx 32rpx 100rpx;
}
.leavelist .no {
  text-align: center;
  margin-top: 200rpx;
  color: #999;
}
.leavelist .leavaItem {
  margin-bottom: 32rpx;
  padding: 18rpx 22rpx;
  background: #ffffff;
  box-shadow: 0px 0px 8px rgba(0, 204, 146, 0.1);
  border-radius: 32rpx;
}
.leavelist .leavaItem .itemHead image {
  width: 48rpx;
  height: 48rpx;
  margin-right: 20rpx;
}
.leavelist .leavaItem .itemHead text {
  color: #808080;
  font-size: 28rpx;
  font-weight: 400;
  margin-right: 20rpx;
}
.leavelist .leavaItem .itemHead .stype {
  color: #00cc92;
  font-size: 24rpx;
  font-weight: 400;
}
.leavelist .leavaItem .itemHead .stype0 {
  color: #FF8D1A;
  font-size: 24rpx;
  font-weight: 400;
}
.leavelist .leavaItem .itemHead .stypes {
  color: #e33c64;
  font-size: 24rpx;
  font-weight: 400;
}
.leavelist .leavaItem .iteminfo {
  margin: 22rpx 0;
}
.leavelist .leavaItem .iteminfo text {
  color: #a6a6a6;
  font-size: 30rpx;
  font-weight: 400;
  margin: 0 20rpx;
}
.leavelist .leavaItem .iteminfo text:first-child {
  color: #0dce97;
  font-weight: 700;
  margin: 0;
}
.leavelist .leavaItem .iteminfo text:last-child {
  color: #383838;
  margin: 0;
}
.leavelist .leavaItem .leavatime .ltimeL {
  color: #a6a6a6;
  font-size: 30rpx;
  font-weight: 400;
}
.leavelist .leavaItem .leavatime .ltimeR {
  color: #383838;
  font-size: 30rpx;
  font-weight: 400;
  white-space: nowrap;
}

