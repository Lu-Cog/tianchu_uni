.header {
  width: 100%;
  box-shadow: 0px 2px 6px rgba(0, 204, 146, 0.1);
}
.header .head {
  position: relative;
  padding: 30rpx;
}
.header .head image {
  position: absolute;
  left: 30rpx;
  width: 50rpx;
  height: 50rpx;
}
.header .head text {
  font-size: 36rpx;
  font-weight: 400;
}
.seach {
  margin: 16rpx auto 36rpx;
  position: relative;
  box-sizing: border-box;
  height: 60rpx;
  border-radius: 16rpx;
  width: 90%;
  border: 1px solid #0dce97;
  display: flex;
  justify-content: flex-start;
}
.seach input {
  padding-left: 16rpx;
  width: 70%;
}
.seach .seachBtn {
  position: absolute;
  right: -1rpx;
  width: 110rpx;
  height: 60rpx;
  background: #0dce97;
  border-radius: 16rpx;
}
.seach .seachBtn image {
  width: 48rpx;
  height: 48rpx;
}
.table {
  padding: 0 32rpx;
}
.table .tabhead {
  background: #fafafa;
}
.table .tabhead text {
  padding: 20rpx 0;
  font-size: 30rpx;
  flex: 1;
  text-align: center;
}
.table .tabTr .tabTrLi text {
  padding: 20rpx 0;
  font-size: 30rpx;
  flex: 1;
  text-align: center;
}
.table .tabTr .tabTrLi text:last-child {
  color: #0dce97;
}
.table .tabTr .info {
  height: 0rpx;
  overflow: hidden;
  padding: 0rpx 50rpx;
  transition: height 0.3s;
}
.table .tabTr .info .infoTitle {
  font-size: 32rpx;
  margin-bottom: 20rpx;
}
.table .tabTr .info .infoItem {
  padding: 10rpx 0;
  font-size: 28rpx;
}
.table .tabTr .hAuto {
  height: 540rpx;
}
.table .tabTr:nth-child(even) {
  background: #f0fcf9;
}

