.header {
  width: 100%;
  box-shadow: 0px 2px 6px rgba(0, 204, 146, 0.1);
}
.header .head {
  position: relative;
  padding: 30rpx;
}
.header .head image {
  position: absolute;
  left: 30rpx;
  width: 50rpx;
  height: 50rpx;
}
.header .head text {
  font-size: 36rpx;
  font-weight: 400;
}
.content {
  padding: 48rpx 32rpx;
  background-color: #f5f5f5;
  min-height: 90vh;
}
.content image {
  width: 100%;
}

