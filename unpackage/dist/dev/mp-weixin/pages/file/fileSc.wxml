<view><view class="header"><view class="status_bar"></view><view class="head flex acen jcen"><image src="../../static/img/back2.png" mode="widthFix" data-event-opts="{{[['tap',[['back']]]]}}" bindtap="__e"></image><text>我的收藏</text><text></text></view></view><view class="filetab flex acen"><text data-event-opts="{{[['tap',[['changeTab',[1]]]]]}}" class="{{[(active==1)?'active':'']}}" bindtap="__e">政策文件</text><text data-event-opts="{{[['tap',[['changeTab',[2]]]]]}}" class="{{[(active==2)?'active':'']}}" bindtap="__e">知识文档</text></view><block wx:if="{{active==1}}"><view class="fileMain"><view class="filelist"><block wx:if="{{list.length<=0}}"><view class="no">暂无文档</view></block><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['download',['$0'],[[['list','',index,'url']]]]]]]}}" class="fileitem flex acen jbtw" bindtap="__e"><view class="item_l flex fCol"><text>{{item.title}}</text><text>{{item.createTime}}</text></view><image src="{{item.fav==1?'../../static/img/ysc.png':'../../static/img/wsc.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['fav',['$0'],[[['list','',index]]]]]]]}}" catchtap="__e"></image></view></block></view></view></block><block wx:if="{{active==2}}"><view class="docuMain"><block wx:if="{{list.length<=0}}"><view class="no">暂无文档</view></block><block wx:else><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['download',['$0'],[[['list','',index,'url']]]]]]]}}" class="docuItem flex acen jbtw" bindtap="__e"><view class="item_l flex acen"><image src="{{item.icon}}" mode="aspectFill"></image><view class="flex fCol"><text>{{item.title}}</text><text>{{item.createTime}}</text></view></view><image src="{{item.fav==1?'../../static/img/ysc.png':'../../static/img/wsc.png'}}" mode="widthFix" data-event-opts="{{[['tap',[['fav',['$0'],[[['list','',index]]]]]]]}}" catchtap="__e"></image></view></block></block></view></block></view>