.no {
  text-align: center;
  margin-top: 200rpx;
  color: #999;
}
.header {
  width: 100%;
  box-shadow: 0px 2px 6px rgba(0, 204, 146, 0.1);
}
.header .head {
  position: relative;
  padding: 30rpx;
}
.header .head image {
  position: absolute;
  left: 30rpx;
  width: 50rpx;
  height: 50rpx;
}
.header .head text {
  font-size: 36rpx;
  font-weight: 400;
}
.filetab {
  padding: 32rpx 32rpx 20rpx;
  box-sizing: border-box;
}
.filetab text {
  color: #383838;
  font-size: 32rpx;
  font-weight: 400;
  transition: all 0.3s;
  margin-left: 20rpx;
}
.filetab .active {
  position: relative;
  z-index: 100;
}
.filetab .active:before {
  content: '';
  z-index: 99;
  position: absolute;
  left: 0;
  bottom: -8rpx;
  height: 10rpx;
  width: 100%;
  border-radius: 4rpx;
  background-color: #0dce97;
}
.fileMain {
  padding: 0rpx 32rpx 48rpx;
  box-sizing: border-box;
}
.fileMain .filelist .fileitem {
  box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.1);
  border-radius: 8px;
  padding: 20rpx;
  margin: 20rpx 0;
  box-sizing: border-box;
}
.fileMain .filelist .fileitem .item_l text {
  color: #383838;
  font-size: 28rpx;
  font-weight: 400;
  word-break: break-all;
}
.fileMain .filelist .fileitem .item_l text:last-child {
  color: #a6a6a6;
  font-size: 24rpx;
}
.fileMain .filelist .fileitem image {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
}
.docuMain {
  padding: 0rpx 32rpx 48rpx;
  box-sizing: border-box;
}
.docuMain .docuItem {
  padding: 20rpx;
  margin: 20rpx 0;
}
.docuMain .docuItem .item_l {
  width: 80%;
}
.docuMain .docuItem .item_l text {
  word-break: break-all;
  margin-left: 20rpx;
  color: #383838;
  font-size: 26rpx;
  font-weight: 400;
}
.docuMain .docuItem .item_l text:last-child {
  color: #a6a6a6;
  font-size: 24rpx;
}
.docuMain .docuItem image {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
}

