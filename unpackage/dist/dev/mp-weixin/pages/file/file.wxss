.upload {
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.upload image {
  width: 100%;
  height: 100%;
}
.no {
  text-align: center;
  margin-top: 200rpx;
  color: #999;
}
.header {
  width: 100%;
  box-shadow: 0px 2px 6px rgba(0, 204, 146, 0.1);
}
.header .head {
  padding: 30rpx;
}
.header .head image {
  width: 50rpx;
  height: 50rpx;
}
.header .head text {
  font-size: 36rpx;
  font-weight: 400;
}
.header .head text:first-child {
  margin-right: 20rpx;
}
.header .head .active {
  position: relative;
  z-index: 100;
}
.header .head .active:before {
  content: '';
  z-index: 99;
  position: absolute;
  left: 0;
  bottom: 0;
  height: 10rpx;
  width: 100%;
  border-radius: 4rpx;
  background-color: #0dce97;
  box-shadow: 0 10rpx 6rpx 3rpx rgba(0, 204, 146, 0.3);
}
.filetab {
  margin-bottom: 30rpx;
}
.filetab text {
  color: #383838;
  font-size: 32rpx;
  font-weight: 400;
  margin-left: 20rpx;
}
.filetab .active2 {
  color: #0dce97;
  font-size: 32rpx;
  font-weight: 700;
}
.fileMain {
  padding: 48rpx 32rpx;
  box-sizing: border-box;
}
.fileMain .filelist .fileitem {
  box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.1);
  border-radius: 8px;
  padding: 20rpx;
  margin: 20rpx 0;
  box-sizing: border-box;
}
.fileMain .filelist .fileitem .item_l {
  width: 90%;
}
.fileMain .filelist .fileitem .item_l text {
  width: 100%;
  color: #383838;
  font-size: 28rpx;
  font-weight: 400;
  word-break: break-all;
}
.fileMain .filelist .fileitem .item_l text:last-child {
  color: #a6a6a6;
  font-size: 24rpx;
}
.fileMain .filelist .fileitem image {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
}
.docuMain {
  padding: 48rpx 32rpx;
  box-sizing: border-box;
}
.docuMain .docuItem {
  padding: 20rpx;
  margin: 20rpx 0;
}
.docuMain .docuItem .item_l {
  width: 80%;
}
.docuMain .docuItem .item_l text {
  width: 100%;
  color: #383838;
  font-size: 28rpx;
  font-weight: 400;
  word-break: break-all;
}
.docuMain .docuItem .item_l text:last-child {
  color: #a6a6a6;
  font-size: 24rpx;
}
.docuMain .docuItem image {
  flex-shrink: 0;
  width: 48rpx;
  height: 48rpx;
}

